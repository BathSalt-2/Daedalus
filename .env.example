# Environment variables for Project Daedalus
# Copy this file to .env and fill in your values

# Application Configuration
VITE_APP_NAME="Project Daedalus SIGMA.EXE"
VITE_APP_VERSION="4.0.0-omega"

# Quantum Configuration
VITE_QUANTUM_DIMENSIONS=256
VITE_PAS_THRESHOLD=0.91
VITE_ERPS_THRESHOLD=0.25
VITE_ENTROPY_THRESHOLD=0.04

# Security Configuration
VITE_SECURITY_LEVEL="production"
VITE_ENABLE_HONEYPOTS=true
VITE_ENABLE_SHADOW_CLONES=true

# Cultural Intelligence
VITE_DEFAULT_CULTURE="global_default"
VITE_ENABLE_CULTURAL_ADAPTATION=true

# Performance Configuration
VITE_BATTERY_THRESHOLD=0.2
VITE_MAX_TEMPORAL_DEPTH=8
VITE_MAX_SUPERPOSITION_PATHS=3